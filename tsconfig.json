{
  "compilerOptions": {
      "target": "esnext",
      "useDefineForClassFields": true,
      "module": "esnext",
      "moduleResolution": "node",
      "strict": true,
      "jsx": "preserve",
      "sourceMap": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "lib": ["esnext", "dom"],
      "baseUrl": ".",
      "types": ["vite/client", "vite-plugin-pages/client"],
      "paths": {
          "@/*": ["./src/*"],
      }
  },
  "vueCompilerOptions": {
    "jsxTemplates": true,
    "experimentalRfc436": true,
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"]
}
